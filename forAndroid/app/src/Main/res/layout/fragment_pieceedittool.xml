    android:layout_height="wrap_content"
    android:layout_margin="0dp"
    android:orientation="horizontal"
    android:padding="0dp"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:id="@+id/toolSelectLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:orientation="horizontal"
        android:padding="0dp">
    </LinearLayout>
    <LinearLayout android:id="@+id/pieceEditToolLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:orientation="horizontal"
        android:padding="0dp">
    </LinearLayout>

</LinearLayout>




